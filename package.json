{
  "name": "python-webpack-boilerplate",
  "version": "1.0.3",
  "description": "Webpack boilerplate for Django & Flask",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config static/webpack/webpack.config.prod.js",
    "start": "webpack serve --config static/webpack/webpack.config.dev.js --host 0.0.0.0 --port 3000 --hot",
    "watch": "webpack --watch --config static/webpack/webpack.config.watch.js",
    "flask-start": "flask run --host=0.0.0.0 --port=8000",
    "dev": "concurrently \"npm run watch\" \"npm run flask-start\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint 'static/src/**/*.js' --quiet",
    "stylelint": "stylelint '**/*.scss'",
    "fix:lint": "eslint 'static/src/**/*.js' --fix",
    "fix:stylelint": "stylelint '**/*.scss' --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AccordBox/python-webpack-boilerplate"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend"
  ],
  "author": "Michael Yin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AccordBox/python-webpack-boilerplate/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.25.7",
    "@swc/core": "^1.9.2",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.1",
    "eslint": "^8.28.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-webpack-plugin": "^4.1.0",
    "mini-css-extract-plugin": "^2.7.1",
    "sass": "~1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.1",
    "stylelint": "^16.4.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-webpack-plugin": "^5.0.0",
    "swc-loader": "^0.2.6",
    "webpack": "^5.95.0",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^5.0.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "core-js": "^3.37.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}